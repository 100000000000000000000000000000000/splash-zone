local args = {
    [1] = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Accessory (Meshes/Aircraft Tow TractorAccessory)"),
    [2] = "SIZE",
    [3] = {
        [1] = "1.5",
        [2] = "1.50",
        [3] = "1.50"
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("AccessoryEditEvent"):FireServer(unpack(args))
